
@-webkit-keyframes roll {
0% {
		-webkit-transform: rotate(0deg) translateY(-13rpx);
		        transform: rotate(0deg) translateY(-13rpx)
}
25% {
		-webkit-transform: rotate(90deg) translateY(-0rpx);
		        transform: rotate(90deg) translateY(-0rpx)
}
50% {
		-webkit-transform: rotate(180deg) translateY(-13rpx);
		        transform: rotate(180deg) translateY(-13rpx)
}
75% {
		-webkit-transform: rotate(270deg) translateY(0rpx);
		        transform: rotate(270deg) translateY(0rpx)
}
100% {
		-webkit-transform: rotate(360deg) translateY(-13rpx);
		        transform: rotate(360deg) translateY(-13rpx)
}
}
@keyframes roll {
0% {
		-webkit-transform: rotate(0deg) translateY(-13rpx);
		        transform: rotate(0deg) translateY(-13rpx)
}
25% {
		-webkit-transform: rotate(90deg) translateY(-0rpx);
		        transform: rotate(90deg) translateY(-0rpx)
}
50% {
		-webkit-transform: rotate(180deg) translateY(-13rpx);
		        transform: rotate(180deg) translateY(-13rpx)
}
75% {
		-webkit-transform: rotate(270deg) translateY(0rpx);
		        transform: rotate(270deg) translateY(0rpx)
}
100% {
		-webkit-transform: rotate(360deg) translateY(-13rpx);
		        transform: rotate(360deg) translateY(-13rpx)
}
}
.rolling-box {
	-webkit-animation: roll 4s infinite linear;
	        animation: roll 4s infinite linear;
	z-index: 989;
	width: 80rpx;
	height: 80rpx;
	-webkit-border-radius: 40rpx;
	        border-radius: 40rpx;
	background-color: rgba(255, 255, 255, 0.3);
	position: absolute;
	top: 90rpx;
	left: 60rpx;
}
.bar-bg {
	background-image: url(data:image/jpeg;base64,);
	background-size: cover;
	background-position: center;
	background-repeat: no-repeat;
	padding-top: 40rpx;
	padding-bottom: 40rpx;
}
.bar-box {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	position: relative;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	min-height: 100rpx;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.bar-text {
	position: absolute;
	text-align: center;
	width: calc(100% - 340rpx);
	left: 0;
	right: 0;
	bottom: 0;
	top: 0;
	margin: auto;
	height: 60rpx;
	font-size: 32rpx;
	line-height: 60rpx;
	cursor: none;
	pointer-events: none;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
	color: #ffffff;
	font-weight: bold;
}
.bar-box.ios .bar-text {
	bottom: 7px;
	height: 30px;
	font-size: 32rpx;
	line-height: 30px;
}
.wave-gif {
	display: block;
	border: none;
	mix-blend-mode: screen;
	height: 100rpx;
	margin-top: -100rpx;
	width: 100%;
}
.wave-box {
	position: relative;
	margin-bottom: -2px;
}
.wave-box::before {
	content: "";
	display: block;
	background: inherit;
	-webkit-filter: blur(10rpx);
	        filter: blur(10rpx);
	position: absolute;
	width: 100%;
	height: 100%;
	top: 10rpx;
	left: 10rpx;
	z-index: -1;
	opacity: 0.4;
	-webkit-transform-origin: 0 0;
	        transform-origin: 0 0;
	-webkit-border-radius: inherit;
	        border-radius: inherit;
	-webkit-transform: scale(1, 1);
	        transform: scale(1, 1);
}

